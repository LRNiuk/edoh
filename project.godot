; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Evil Dream of Hechiz"
run/main_scene="res://Base/Intro.tscn"
boot_splash/image="res://Sprites/title.png"
boot_splash/fullsize=false
config/icon="res://Sprites/icons/icon256.png"

[autoload]

STAGEGLOBAL="*res://Base/STAGEGLOBAL.gd"

[debug]

settings/crash_handler/message="An error has ocurred! Please report how this happened to mikibox."

[display]

window/size/width=480
window/size/height=800
window/size/resizable=false
window/energy_saving/keep_screen_on=false
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/shrink="1"
window/handheld/emulate_touchscreen=true

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/shading/force_vertex_shading.mobile=false
quality/depth_prepass/enable=false
quality/subsurface_scattering/weight_samples=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
